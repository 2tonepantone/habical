<section>
  <h5>Next 10 events</h5>
  <table class="">
    <thead>
      <tr>
        <th class="">Time</th>
        <th class="">Summary</th>
      </tr>
    </thead>
    <tbody>
      <% @result.items.each do |event| %>
        <tr>
          <td class=""><%= event.start.date_time || event.start.date %></td>
          <td class=""><%= event.summary %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <h5>Busy Times</h5>
  <table class="">
    <thead>
      <tr>
        <th class="">Start Time</th>
        <th class="">End Time</th>
      </tr>
    </thead>
    <tbody>
      <% @busy_times.each do |event| %>
        <tr>
          <td class=""><%= event.start %></td>
          <td class=""><%= event.end %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</section>
